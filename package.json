{
  "name": "split-json",
  "version": "2.0.0",
  "description": "split text stream (JSON sent through some kind of readable stream) into JSON objects",
  "main": "index.js",
  "directories": {
    "test": "test"
    "lib": "lib"
  },
  "files": [
    "index.js"
    "lib",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "test": "istanbul cover tape test/index.js",
    "jshint": "jshint -c .jshintrc --exclude-path .gitignore .",
    "code-style": "jscs -p google lib/*.js test/*.js",
    "check-coverage": "istanbul check-coverage --statements 100 --functions 100 --lines 100 --branches 100",
    "open-coverage": "open coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joaquimserafim/split-json.git"
  },
  "keywords": [
    "split",
    "stream",
    "JSON"
  ],
  "engines": {
    "node": ">=0.10"
  },
  "author": "@joaquimserafim",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/joaquimserafim/split-json/issues"
  },
  "homepage": "https://github.com/joaquimserafim/split-json",
  "dependencies": {
    "is-js-object": "^1.0.0",
    "is-regexp": "^1.0.0",
    "json-parse-safe": "^1.0.3",
    "through2": "^0.6.3"
  },
  "devDependencies": {
    "istanbul": "^0.3.8",
    "jscs": "^1.11.3",
    "jshint": "^2.6.3",
    "pre-commit": "^1.0.6",
    "tape": "^3.5.0"
  },
  "pre-commit": [
    "jshint",
    "code-style",
    "check-coverage",
    "test"
  ]
}
